{
  "name": "pantra",
  "version": "0.1.1",
  "description": "Kitchen food stock manager",
  "author": "Charles Stello",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "axios": "^1.4.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.11.2",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dev": "concurrently -k \"set BROWSER=none&&npm start\" \"npm run electron\"",
    "electron": "wait-on tcp:127.0.0.1:3000 && electron-forge start",
    "package": "react-scripts build && electron-forge package",
    "make": "react-scripts build && electron-forge make",
    "make-all": "react-scripts build && electron-forge make --platform=win32 --platform=darwin --platform=linux"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-dmg": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "concurrently": "^8.1.0",
    "electron": "^25.0.1",
    "electron-devtools-installer": "^3.2.0",
    "wait-on": "^7.0.1"
  }
}
